{
    "manifest_version": 3,
    "name": "Alura Rank",
    "description": "Monsta seu Ranking",
    "version": "3.3",
    "author": "Luis Dias",
    "action": {
        "default_title": "Ranking",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "host_permissions": [        
        "*://*.alura.com.br/*"
    ]
}

